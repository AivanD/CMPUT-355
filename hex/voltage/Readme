6e = b+c+d+f+g+h,
4f = c+e+h+i,
3g = d+e,
3h = e+f,
2i = f

r = (u+1.0)/2.0
s = (u+v+1.0)/3.0
t = (v+w+1.0)/3.0
u = (r+s+v+x)/4.0
v = (s+t+u+w+x+y)/6.0
w = (t+v+y+z)/4.0
x = (u+v)/3.0
y = (v+w)/3.0
z = (w)/2.0

